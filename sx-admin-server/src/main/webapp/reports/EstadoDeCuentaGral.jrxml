	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="FECHA" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT A.CLIENTE,A.NOMBRE,SUM(A.TOTAL) AS TOTAL,SUM(A.SALDO) AS SALDO,SUM(A.X_VENCER) AS X_VENCER,SUM(A.VENCIDO) AS VENCIDO
,MAX(A.ATR) AS ATR_MAX,SUM(A.IMP_CAR) AS IMP_CAR,SUM(A.SALDO) + SUM(A.IMP_CAR) AS SALDO_CARGO
FROM (
SELECT C.CLAVE AS CLIENTE,C.NOMBRE,S.NOMBRE AS SUCURSAL,(CASE 	WHEN V.cheque_post_fechado IS TRUE THEN 'PSF' WHEN V.TIPO_DOCUMENTO='VENTA' THEN 'FAC' ELSE 'CAR' END) as OPERACION,V.DOCUMENTO,V.FECHA
,V.TOTAL*V.TIPO_DE_CAMBIO AS TOTAL,(V.TOTAL-IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B WHERE B.cuenta_por_cobrar_id=V.ID AND B.fecha<=$P{FECHA}),0))*V.TIPO_DE_CAMBIO AS SALDO 
,(CASE WHEN TO_DAYS($P{FECHA})-TO_DAYS(V.VENCIMIENTO)>0 THEN TO_DAYS(CURRENT_DATE)-TO_DAYS(V.VENCIMIENTO) ELSE 0 END) AS ATR
,(CASE WHEN (TO_DAYS($P{FECHA})-TO_DAYS(V.VENCIMIENTO))<=0 THEN (V.TOTAL-IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B WHERE B.cuenta_por_cobrar_id=V.ID AND B.fecha<=$P{FECHA} ),0))*V.TIPO_DE_CAMBIO ELSE 0 END) AS X_VENCER
,(CASE WHEN (TO_DAYS($P{FECHA})-TO_DAYS(V.VENCIMIENTO))>0 THEN (V.TOTAL-IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B WHERE B.cuenta_por_cobrar_id=V.ID AND B.fecha<=$P{FECHA} ),0))*V.TIPO_DE_CAMBIO ELSE 0 END) AS VENCIDO
,(IFNULL((CASE WHEN TRUNCATE((TO_DAYS($P{FECHA})-TO_DAYS(DATE(DATE_ADD(V.FECHA, INTERVAL X.PLAZO DAY))))/7,0)<=0 THEN 0 ELSE TRUNCATE((TO_DAYS($P{FECHA})-TO_DAYS(DATE(DATE_ADD(V.FECHA, INTERVAL X.PLAZO DAY))))/7,0) END),0)
*((V.TOTAL)-IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B JOIN COBRO Y ON(Y.ID=B.cobro_id) WHERE B.cuenta_por_cobrar_id=V.ID AND B.fecha<=$P{FECHA} AND Y.FORMA_DE_PAGO='DEVOLUCION'),0))/100)*V.tipo_De_cambio AS IMP_CAR
,(case when j.traspaso is not null and date(j.traspaso)<=$P{FECHA} then 'JUR' else v.tipo end) ORIGEN
FROM cuenta_por_cobrar V JOIN CLIENTE C ON(V.CLIENTE_ID=C.ID) LEFT JOIN CLIENTE_CREDITO X ON(X.CLIENTE_ID=C.ID) JOIN sucursal S ON(V.SUCURSAL_ID=S.ID)  left join juridico j on(j.cxc_id=v.id)
WHERE V.FECHA>'2016/12/31' AND V.FECHA <=$P{FECHA}  AND V.TOTAL-IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B WHERE B.CUENTA_POR_COBRAR_ID=V.ID AND B.fecha<=$P{FECHA}),0)<>0 
UNION
SELECT C.CLAVE AS CLIENTE,C.NOMBRE,S.NOMBRE AS SUCURSAL,SUBSTR(V.forma_de_pago,1,3) as OPERACION,V.REFERENCIA AS NUMERO,V.FECHA,-V.IMPORTE*V.TIPO_DE_CAMBIO AS TOTAL
,(V.IMPORTE-V.DIFERENCIA-IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B WHERE B.cobro_id=V.ID AND B.fecha<=$P{FECHA}),0))*V.TIPO_DE_CAMBIO*-1 AS SALDO,0 AS ATR,0 AS XVENCER,0 AS VENCIDO,0 AS IMP_CAR
,(case when ifnull((select max(j.traspaso) from cliente c join juridico j on(j.nombre=c.nombre) where v.cliente_id=c.id),DATE(DATE_ADD('2019-06-11', INTERVAL 1 DAY)))<=$P{FECHA} then 'JUR' else v.tipo end) ORIGEN 
FROM cobro V JOIN CLIENTE C ON(V.CLIENTE_ID=C.ID) LEFT JOIN CLIENTE_CREDITO X ON(X.CLIENTE_ID=C.ID) JOIN sucursal S ON(V.SUCURSAL_ID=S.ID) 
WHERE V.FECHA>'2015/09/01' AND V.FECHA <=$P{FECHA}  AND V.forma_de_pago in('DEVOLUCION','BONIFICACION') 

AND V.IMPORTE-V.DIFERENCIA-IFNULL((SELECT SUM(B.IMPORTE) FROM aplicacion_de_cobro B WHERE B.COBRO_ID=V.ID AND B.fecha<=$P{FECHA}),0)<>0
) AS A WHERE A.ORIGEN='CRE'  GROUP BY A.CLIENTE]]></queryString>

